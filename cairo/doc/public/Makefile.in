@ENABLE_GTK_DOC_TRUE@scan-build.stamp: $(HFILE_GLOB) $(CFILE_GLOB)
@ENABLE_GTK_DOC_TRUE@	@echo 'gtk-doc: Scanning header files'
@ENABLE_GTK_DOC_TRUE@	@-chmod -R u+w $(srcdir)
